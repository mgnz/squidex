<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" >
      <environmentVariables>
        <environmentVariable xdt:Transform="SetAttributes" xdt:Locator="Match(name)" name="ASPNETCORE_ENVIRONMENT" value="production" />
      </environmentVariables>
    </aspNetCore>
  </system.webServer>
</configuration>